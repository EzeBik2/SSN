@using RSF.Models
@model Cancha

<br />
<br />
<br />
<br />
<br />
<br />

<table class="table table-hover" style="width: 700px; float: right;">
    <thead class="thead-inverse">
        <tr>
            <th>Id</th>
            <th>Cancha</th>
            <th>Barrio</th>
            <th>#</th>
        </tr>
    </thead>
    @foreach (Cancha canchaa in ViewBag.A)
    {
        <tr>
            <td>@Html.DisplayTextFor(a => canchaa.id)</td>
            <td>@Html.DisplayTextFor(a => canchaa.nombre)</td>
            <td>@Html.DisplayTextFor(a => canchaa.barrio)</td>
            <td>@Html.ActionLink("VER PERFIL", "BuscarCanchas", "Home", canchaa, null)</td>
        </tr>
    }
</table>

