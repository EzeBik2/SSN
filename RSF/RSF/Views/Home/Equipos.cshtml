@using RSF.Models
@model Equipo

<br />
<br />
<br />
<br />
<br />
<br />

<table class="table table-hover" style="width: 700px; float: right;">
    <thead class="thead-inverse">
        <tr>
            <th>Id</th>
            <th>Cancha</th>
            <th>Barrio</th>
            <th>#</th>
        </tr>
    </thead>
    @foreach (Equipo equipoo in ViewBag.A)
    {
        <tr>
            <td>@Html.DisplayTextFor(a => equipoo.id)</td>
            <td>@Html.DisplayTextFor(a => equipoo.nombre)</td>
            <td>@Html.DisplayTextFor(a => equipoo.cantjug)</td>
            <td>@Html.ActionLink("VER PERFIL", "BuscarEquipos", "Home", equipoo, null)</td>
        </tr>
    }
</table>

