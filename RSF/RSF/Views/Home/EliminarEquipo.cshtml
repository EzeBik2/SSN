@using RSF.Models
<head>
    <script type="text/javascript" src="~/Content/jquery-3.2.1.min.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head> <!-- Agrego JQUERY Y DISEÑO RESPONSIVE-->

@model EquipoJugador

<form method="POST" action="@Url.Action("EliminarEquipo2", "Home")">
    
    @Html.ValidationMessageFor(x => x.idEquipo)<br />
    Id Del Equipo: @Html.TextBoxFor(x => x.idEquipo, new { @class = "form-control" }) <br />

    @Html.ValidationMessageFor(x => x.idJugador)<br />
    Id Del Jugador: @Html.TextBoxFor(x => x.idJugador, new { @class = "form-control" }) <br />

    <input type="submit" value="Eliminar" class="btn btn-primary" />

</form>
